{
  "@context": {
    "dct": "http://purl.org/dc/terms/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfa": "http://www.w3.org/ns/rdfa#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "cvisb": "https://data.cvisb.org/jsonschema/"
  },
  "version": 0.1,
  "@id": "https://data.cvisb.org/jsonschema/v0.1",
  "@graph": [
    {
      "@id": "cvisb:PiccoloMeasurement",
      "@type": "rdfs:Class",
      "rdfs:comment": "A schema describing a set of blood chemistry measurements at a particular timepoint for a patient, collected using a Piccolo Xpress analyzer in the Center for Viral Systems Biology.",
      "rdfs:label": "PiccoloMeasurement",
      "rdfs:subClassOf": {
        "@id": "cvisb:Data"
      },
      "schema:isPartOf": {
        "@id": "https://data.cvisb.org/schema"
      }
    },
    {
      "@id": "cvisb:timepoint",
      "@type": "rdf:Property",
      "rdfs:comment": "Description of when the Piccolo Measurement was taken(e.g. at survivor enrollment, initial presentation in hospital, etc.)",
      "rdfs:label": "timepoint",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PiccoloMeasurement"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ],
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": true
    },
    {
      "@id": "cvisb:daysSinceAdmit",
      "@type": "rdf:Property",
      "rdfs:comment": "Number of days between the admission date and when Piccolo data was measured. At initial clinical presentation, daysSinceAdmit = 0",
      "rdfs:label": "daysSinceAdmit",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PiccoloMeasurement"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "schema:Integer"
        }
      ],
      "owl:cardinality": "one",
      "marginality": "recommended",
      "authenticated": true
    },
    {
      "@id": "cvisb:piccolo",
      "@type": "rdf:Property",
      "rdfs:comment": "Object of Piccolo data measured for the patient at the timepoint.",
      "rdfs:label": "piccolo",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PiccoloMeasurement"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "cvisb:PiccoloData"
        }
      ],
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": true
    },
    {
      "@id": "cvisb:Place",
      "@type": "rdfs:Class",
      "rdfs:comment": "A schema describing a geographic Place in the Center for Viral Systems Biology. Used typically to specify exact GPS coordinates",
      "rdfs:label": "Place",
      "rdfs:subClassOf": {
        "@id": "schema:Place"
      },
      "schema:isPartOf": {
        "@id": "https://data.cvisb.org/schema"
      },
      "schema:sameAs": {
        "@id": "schema:Place"
      }
    },
    {
      "@id": "cvisb:latitude",
      "@type": "rdf:Property",
      "rdfs:comment": "Latitude of the place. For administrative units, will be the coordinate of the polygon centroid.",
      "rdfs:label": "latitude",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:Place"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "schema:Number"
        }
      ],
      "owl:cardinality": "one",
      "marginality": "recommended"
    },
    {
      "@id": "cvisb:longitude",
      "@type": "rdf:Property",
      "rdfs:comment": "Longitude of the place. For administrative units, will be the coordinate of the polygon centroid.",
      "rdfs:label": "longitude",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:Place"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "schema:Number"
        }
      ],
      "owl:cardinality": "one",
      "marginality": "recommended"
    },
    {
      "@id": "cvisb:containedInPlace",
      "@type": "rdf:Property",
      "rdfs:comment": "Additional administrative units that the GPS coordinates are located within",
      "rdfs:label": "containedInPlace",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:Place"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "cvisb:AdministrativeArea"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:containedInPlace"
      },
      "owl:cardinality": "one",
      "marginality": "optional"
    },
    {
      "@id": "cvisb:PostalAddress",
      "@type": "rdfs:Class",
      "rdfs:comment": "A schema describing an PostalAddress for the Center for Viral Systems Biology",
      "rdfs:label": "PostalAddress",
      "rdfs:subClassOf": {
        "@id": "schema:PostalAddress"
      },
      "schema:isPartOf": {
        "@id": "https://data.cvisb.org/schema"
      },
      "schema:sameAs": {
        "@id": "schema:PostalAddress"
      }
    },
    {
      "@id": "cvisb:streetAddress",
      "@type": "rdf:Property",
      "rdfs:comment": "The street address. For example, 1600 Amphitheatre Pkwy",
      "rdfs:label": "streetAddress",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PostalAddress"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "xsd:string"
        },
        {
          "@id": "schema:Text"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:streetAddress"
      },
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": false
    },
    {
      "@id": "cvisb:addressLocality",
      "@type": "rdf:Property",
      "rdfs:comment": "The locality (e.g. city). For example, Mountain View.",
      "rdfs:label": "addressLocality",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PostalAddress"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "xsd:string"
        },
        {
          "@id": "schema:Text"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:addressLocality"
      },
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": false
    },
    {
      "@id": "cvisb:addressRegion",
      "@type": "rdf:Property",
      "rdfs:comment": "The region (e.g. state/Adm 2). For example, CA.",
      "rdfs:label": "addressRegion",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PostalAddress"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "xsd:string"
        },
        {
          "@id": "schema:Text"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:addressRegion"
      },
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": false
    },
    {
      "@id": "cvisb:postalCode",
      "@type": "rdf:Property",
      "rdfs:comment": "The postal code. For example, 94043.",
      "rdfs:label": "postalCode",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PostalAddress"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "xsd:string"
        },
        {
          "@id": "schema:Text"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:postalCode"
      },
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": false
    },
    {
      "@id": "cvisb:addressCountry",
      "@type": "rdf:Property",
      "rdfs:comment": "The country object (containing name, ISO code/identifier, link to ISO page)",
      "rdfs:label": "addressCountry",
      "schema:domainIncludes": [
        {
          "@id": "cvisb:PostalAddress"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "cvisb:Country"
        }
      ],
      "schema:sameAs": {
        "@id": "schema:addressCountry"
      },
      "owl:cardinality": "one",
      "marginality": "required",
      "authenticated": false
    }
  ]
}